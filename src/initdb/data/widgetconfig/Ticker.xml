<?xml version="1.0" encoding="UTF-8"?>
<widgetConfiguration displayFlag="false" height="1.2em" type="Ticker" title="!{lb_Ticker}" backgroundColor="#FFFFFF">
	<WidgetPref datatype="string" display_name="!{lb_dateTimeFormat}" name="dateTimeFormat" value="yyyy/MM/dd HH:mm:ss"/>
	<UserPref default_value="" datatype="hidden" name="url" display_name="RSS URL" required="true" admin_datatype="url"/>
	<UserPref default_value="false" display_name="!{lb_doShowDatetime}" datatype="bool" name="date"/>
	<UserPref default_value="true" display_name="!{lb_displayTitle}" datatype="bool" name="title"/>
	<UserPref default_value="false" display_name="!{lb_descDisplay}" datatype="bool" name="desc"/>
	<UserPref default_value="2" display_name="!{lb_displayspeed}" datatype="radio" name="speed">
		<EnumValue display_value="1" value="1"/>
		<EnumValue display_value="2" value="2"/>
		<EnumValue display_value="3" value="3"/>
		<EnumValue display_value="4" value="4"/>
		<EnumValue display_value="5" value="5"/>
	</UserPref>
	<UserPref default_value="1" display_name="!{lb_reacquisitionIntervalMinute}" datatype="string" name="refreshInterval"/>
	<Content className="Ticker" type="javascript"/>
</widgetConfiguration>
