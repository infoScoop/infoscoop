<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="AccountAttr"
		table="IS_ACCOUNT_ATTRS"
	>
		<meta attribute="sync-DAO">false</meta>
        <id
                name="Id"
                type="java.lang.Long"
                column="ID"
        >
            <generator class="native">
                <param name="sequence">is_account_attrs_id_seq</param>
            </generator>
        </id>

        <property
            name="accountId"
            type="string"
            column="fk_account_id"
            length="150"
        />

		<property
			name="name"
			column="NAME"
			type="string"
			length="255"
		/>

        <property
            name="value"
            column="value"
            type="string"
            not-null="false"
        />

        <property
                column="system"
                name="system"
                type="boolean"
         />

		<property
				name="squareId"
				column="square_id"
				type="string"
				not-null="false"
				length="64"
		/>
	</class>
</hibernate-mapping>