<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
			name="GlobalPreference"
			table="is_globalpreferences"
			>
		<meta attribute="sync-DAO">false</meta>
		<id
				name="Id"
				type="java.lang.Long"
				column="ID"
		>
			<generator class="native">
				<param name="sequence">is_globalpreferences_id_seq</param>
			</generator>
		</id>

        <property
            name="Uid"
            column="`UID`"
            type="string"
            not-null="false"
            length="150"
        />
        <property
            name="Name"
            column="name"
            type="string"
            length="256"
        />
        <property
            name="Value"
            column="value"
            type="string"
            length="512"
        />
	</class>
</hibernate-mapping>