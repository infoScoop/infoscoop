<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.infoscoop.dao.model">
    <class name="Notification" table="is_notifications">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="squareId" type="string">
            <column name="SQUARE_ID" length="64" not-null="true" />
        </property>
        <property name="type" type="string">
            <column name="TYPE" length="32" not-null="true" />
        </property>
        <property name="body" type="string">
            <column name="BODY" not-null="false" />
        </property>
        <property name="lastmodified" type="timestamp">
            <column name="LASTMODIFIED" not-null="true" />
        </property>
        <many-to-one class="org.infoscoop.dao.model.Square"
            name="square"
            access="field"
            cascade="none"
            update="false"
            insert="false"
            outer-join="auto"
            not-found="ignore">
            <column name="SQUARE_ID"/>
        </many-to-one>
    </class>
</hibernate-mapping>
