<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="TabLayout"
		table="IS_TABLAYOUTS"
	>
		<meta attribute="sync-DAO">false</meta>

		<composite-id name="Id" class="TABLAYOUTPK">
			<key-property
				name="Tabid"
				column="TABID"
				type="string"
			/>
			<key-property
				name="Roleorder"
				column="ROLEORDER"
				type="integer"
			/>
			<key-property
				name="Temp"
				column="TEMP"
				type="integer"
			/>
		</composite-id>

		<property
			name="Role"
			column="ROLE"
			type="text"
			not-null="true"
		/>
		<property
			name="Rolename"
			column="ROLENAME"
			type="string"
			not-null="true"
			length="256"
		/>
		<property
			name="Principaltype"
			column="PRINCIPALTYPE"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="Defaultuid"
			column="DEFAULTUID"
			type="string"
			not-null="false"
			length="150"
		/>
		<!-- please tell Joe Hudson that the type 'XML' could not be resolved.. defaulting to java.lang.String -->
		<property
			name="Widgets"
			column="WIDGETS"
			type="text"
			not-null="true"
		/>
		<property
			name="layout"
			column="LAYOUT"
			type="text"
			access="field"
		/>
		<property
			name="Widgetslastmodified"
			column="WIDGETSLASTMODIFIED"
			type="string"
			not-null="true"
			length="24"
		/>
		<property
			name="Workinguid"
			column="WORKINGUID"
			type="string"
			not-null="false"
			length="150"
		/>
		<property
			name="Templastmodified"
			column="TEMPLASTMODIFIED"
			type="timestamp"
			not-null="false"
			length="26"
		/>
		
	    <many-to-one
	    	name="statictab"
	    	class="StaticTab"
	    	insert="false"
	    	not-found="ignore">
	      <column name="Tabid" />
	    </many-to-one>
		<!-- 
		<many-to-one class="org.infoscoop.dao.model.StaticTab"
			name="statictab"
			access="field"
			cascade="none"
			outer-join="false"
			column="Tabid"
			property-ref="Tabid"
			not-found="ignore"/>
		-->
		<!-- 
		<set inverse="true" name="TabAdmin" sort="unsorted">
			<key column="Tabid"/>
			<many-to-many class="TabAdmin"/>
		</set>
		 -->
	</class>	
</hibernate-mapping>