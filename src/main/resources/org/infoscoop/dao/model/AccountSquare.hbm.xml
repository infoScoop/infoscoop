<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="AccountSquare"
		table="IS_ACCOUNT_SQUARES"
	>
		<meta attribute="sync-DAO">false</meta>
        <id
                name="Id"
                type="java.lang.Long"
                column="ID"
        >
            <generator class="native">
                <param name="sequence">is_account_squares_id_seq</param>
            </generator>
        </id>

		<property
			name="accountId"
			type="string"
			column="fk_account_id"
			length="150"
		/>

        <property
            name="squareId"
            column="square_id"
            type="string"
            not-null="false"
            length="64"
        />

		<property name="created" type="timestamp">
			<column name="CREATED" not-null="true" />
		</property>
	</class>
</hibernate-mapping>