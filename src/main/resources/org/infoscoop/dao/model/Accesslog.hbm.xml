<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Accesslog"
		table="IS_ACCESSLOGS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native">
				<param name="sequence">is_accesslogs_id_seq</param>
			</generator>
		</id>

		<property
			name="Uid"
			column="`UID`"
			type="string"
			not-null="true"
			length="150"
		/>
		<property
			name="Date"
			column="`DATE`"
			type="string"
			not-null="true"
			length="8"
		/>
        <property
            name="Squareid"
            column="square_id"
            type="string"
            not-null="true"
            length="64"
        />

	</class>	
</hibernate-mapping>