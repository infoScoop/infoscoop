<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="I18nlastmodified"
		table="IS_I18NLASTMODIFIED"
	>
		<meta attribute="sync-DAO">false</meta>
        <composite-id name="Id" class="I18NlastmodifiedPK">
            <key-property
                name="Id"
                column="TYPE"
                type="string"
            />
            <key-property
                name="Squareid"
                column="square_id"
                type="string"
                length="64"
            />
        </composite-id>

		<property
			name="Lastmodified"
			column="LASTMODIFIED"
			type="timestamp"
			not-null="false"
			length="26"
		/>


	</class>	
</hibernate-mapping>