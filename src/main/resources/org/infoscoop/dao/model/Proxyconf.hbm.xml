<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Proxyconf"
		table="IS_PROXYCONFS"
	>
		
        <composite-id name="Id" class="ProxyconfPK">
            <key-property
                name="Temp"
                column="TEMP"
                type="integer"
            />
            <key-property
                name="Squareid"
                column="square_id"
                type="string"
                length="64"
            />
        </composite-id>
		
		
		<property
			name="Data"
			column="DATA"
			type="java.lang.String"
			not-null="true"
		/>
		<property
			name="Lastmodified"
			column="LASTMODIFIED"
			type="timestamp"
			not-null="false"
			length="26"
		/>


	</class>	
</hibernate-mapping>