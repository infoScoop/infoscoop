<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="TabAdmin"
		table="is_tab_admins"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="Id" class="TabAdminPK">
			<key-property
				name="Tabid"
				column="TABID"
				type="string"
			/>
			<key-property
				name="Uid"
				column="adminUid"
				type="string"
				length="150"
			/>
            <key-property
                name="Squareid"
                column="square_id"
                type="string"
                length="64"
            />
		</composite-id>
		
	</class>	
</hibernate-mapping>