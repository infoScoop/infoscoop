<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Adminrole"
		table="IS_ADMINROLES"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
			<generator class="native">
				<param name="sequence">is_adminRoles_id_seq</param>
			</generator>
		</id>

		<property
			name="Roleid"
			column="ROLEID"
			type="string"
			not-null="true"
			length="256"
		/>
		<property
			name="Name"
			column="NAME"
			type="string"
			not-null="true"
			length="256"
		/>
		<property
			name="Permission"
			column="PERMISSION"
			type="string"
			not-null="true"
			length="256"
		/>
		<property
			name="Allowdelete"
			column="ALLOWDELETE"
			type="integer"
			not-null="false"
			length="10"
		/>
        <property
            name="Squareid"
            column="square_id"
            type="string"
            not-null="true"
            length="64"
        />

		<set name="Portaladmins" inverse="true">
			<key>
				<column name="Roleid"/>
			</key>
			<one-to-many class="Portaladmins"/>
		</set>


	</class>	
</hibernate-mapping>