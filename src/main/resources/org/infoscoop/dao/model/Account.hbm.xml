<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Account"
		table="IS_ACCOUNTS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Uid"
			type="string"
			column="`UID`"
			length="150"
		/>

		<property
			name="name"
			column="NAME"
			type="string"
			length="255"
			not-null="false"
		/>

		<property
			name="password"
			column="PASSWORD"
			type="string"
			length="255"
			not-null="false"
		/>

        <property
            name="defaultSquareId"
            column="default_square_id"
            type="string"
            not-null="false"
            length="64"
        />

		<property
				name="givenName"
				column="given_name"
				type="string"
				not-null="false"
				length="128"
				/>

		<property
				name="familyName"
				column="family_name"
				type="string"
				not-null="false"
				length="128"
				/>

		<property
				name="mail"
				column="email"
				type="string"
				not-null="false"
				length="150"
				/>

		<set name="AccountAttrs" lazy="true" inverse="true">
			<key>
				<column name="uid" not-null="true" />
			</key>
			<one-to-many class="AccountAttr" />
		</set>

		<set name="AccountSquares" lazy="true" inverse="true">
			<key>
				<column name="uid" not-null="true" />
			</key>
			<one-to-many class="AccountSquare" />
		</set>
	</class>
</hibernate-mapping>