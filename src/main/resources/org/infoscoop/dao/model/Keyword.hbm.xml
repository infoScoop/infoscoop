<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Keyword"
		table="IS_KEYWORDS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native">
				<param name="sequence">is_keywords_id_seq</param>
			</generator>
		</id>

		<property
			name="Uid"
			column="`UID`"
			type="string"
			not-null="true"
			length="150"
		/>
		<property
			name="Type"
			column="TYPE"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Keyword"
			column="KEYWORD"
			type="string"
			not-null="true"
			length="500"
		/>
		<property
			name="Date"
			column="`DATE`"
			type="string"
			not-null="true"
			length="24"
		/>
        <property
            name="Squareid"
            column="square_id"
            type="string"
            not-null="true"
            length="64"
        />


	</class>	
</hibernate-mapping>